; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Goco-8"
config/description="Fantasy Console"
run/main_scene="res://Editor.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://icon.png"

[audio]

mix_rate=22050

[autoload]

ES="*res://ES.gd"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/shadowed_variable=false

[display]

window/size/viewport_width=320
window/size/viewport_height=240
window/size/window_width_override=960
window/size/window_height_override=720
window/stretch/mode="viewport"
mouse_cursor/custom_image="res://UI/cursor_default.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/color-palette/plugin.cfg", "res://addons/godot_sfxr/plugin.cfg")

[gui]

theme/custom="res://UI/Theme.tres"
common/drop_mouse_on_gui_input_disabled=true

